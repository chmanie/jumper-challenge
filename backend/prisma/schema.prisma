// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String @id
  tokens     Token[]
}

model Token {
  id         String @id
  name       String?
  symbol     String?
  decimals   Int?
  logo       String?
  users      User[]
}
